import java.util.Scanner;
public class Task_5
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    Systeem.out.println("Please give me a number");
    int num=sc.nextInt();
    int sum=0;
    for(int i=1;i<=num;i++)
    {
      sum=sum+(i*i*i);
    }
    System.out.println("Y="+sum);
  }
}