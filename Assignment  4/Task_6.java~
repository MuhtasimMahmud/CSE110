import java.util.Scanner;
public class Task6
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int num=sc.nextInt();
    int sum=0;
    for(int i=1;i<=num;i++)
    {
      if(i%2!=0)
      {
        sum=sum+(i*i);
      }
      else
      {
        sum=(-1)*sum+(i*i);
      }
    }
    System.out.println("Y="+sum);
    sc.close();
  }
}