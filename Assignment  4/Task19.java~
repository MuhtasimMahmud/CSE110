import java.util.Scanner;
public class Task19
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int num=sc.nextInt();
    int temp=1;
    for(int i=0;i<num;i++)
    {
      num=temp*10;
      num=temp;
    }
    System.out.println(num);
    sc.close();
  }
}