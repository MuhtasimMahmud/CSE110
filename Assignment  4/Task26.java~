import java.util.Scanner;
public class Task26
{
  public static void main(String[]args)
  {
    scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int num=sc.nextInt();
    int counter=0;
    for(int i=1;i<=num;i++)
    {
      if(num%i==0)
      {
        counter++;
      }
    }
    if(counter==2)
    {
      System.out.println("Prime number");
    }
    else
    {
      System.out.println("Not Prime number");
    }
  }
}