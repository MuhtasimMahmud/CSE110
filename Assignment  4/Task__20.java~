import java.util.Scanner;
public class Task__20 //eta for loop die korsi
{
  public static void main(String[p]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.prinbtln("Please give me a number");
    int num=sc.nextInt();
    for(int temp=num;temp>0;num%=10)
    {
      System.out.println(temp);
      temp=temp/10;
    }
  }
}  