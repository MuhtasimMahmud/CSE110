import java.util.Scanner;
public class Task3
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int num=sc.nextInt();
    int max=num;
    for(int i=1;i<20;i++)
    {
      System.out.println("Please give me a number");
      int num=sc.nextInt();
      if(num>=max)
      {
        max=num;
      }
    }
    System.out.println("Maximum number="+max);
  }
}