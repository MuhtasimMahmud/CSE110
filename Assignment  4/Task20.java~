import java.util.Scanner;
public class Task20
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int num=sc.nextInt();
    for(int temp=num;num>0;num%=10)
    {
      System.out.println(num);
      num=num/10;
    }
  }
}