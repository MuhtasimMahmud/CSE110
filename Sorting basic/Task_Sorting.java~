public class Task_Sorting
{
  public static void main(String[]args)
  {
    int a[]=new int[]{10,30,20,50,40};
    int max=a[0];
    for(int i=0;i<a.length;i++)
    {
      if(a[i]>max)
      {
        max=a[i];
      }
      int backup=a[0];
      a[0]=max;
      max=bakup;
    }
    for(int i=0;i<a.length;i++)
    {
      System.out.println(a[i]);
    }
  }
}