import java.util.Scanner;
public class Task11
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    int a[]=new int[10];
    for(int i=0;i<a.length;i++)
    {
      System.out.println("Please give me a number");
      a[i]=sc.nextInt();
      if(a[i]==a[i+1])
      {
        System.out.println("Please give another num");
      }
    }
  }
}