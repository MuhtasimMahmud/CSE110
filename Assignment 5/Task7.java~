public class Task7
{
  public static void main(String[]args)
  {
    int a[]=new int []{10,30,20,50,40};
    for(int i=0;i<a.length;i++)
    {
      int max=i;
      for(int j=i+1;j,a.length;i++)
      {
        if(a[j]>a[max])
        {
          max=j;
        }
      }
      int temp=a[i];
      a[i]=a[max];
      a[max]=temp;
      for(int i=0;i<a.length;i++)
      {
        System.out.print(a[i]+" ");
      }
    }
  }
}